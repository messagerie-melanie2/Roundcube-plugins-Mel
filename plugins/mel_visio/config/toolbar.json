{
  "toolbar": {
    "id": "visio-toolbar",
    "shape": "rectangle:x60px, y0, wcalc(100% - 60px), h60px",
    "color": "var:bureau-card-background-color",
    "icon_type": "material-symbol",
    "buttons": {
      "documents": [
        {
          "icon": "folder_open",
          "text": "mel_metapage.app_documents"
        },
        {
          "icon": "description",
          "text": "mel_metapage.pad",
          "data": {
            "mef": true
          }
        }
      ],
      "chat": {
        "icon": "chat",
        "text": "mel_metapage.chat",
        "data": {
          "disabled-icon": "chat_bubble",
          "has-unread-icon": "mark_unread_chat_alt",
          "has-unread-disabled-icon": "mark_chat_unread"
        }
      },
      "mic": [
        {
          "icon": "mic",
          "text": "mel_metapage.audioinput",
          "data": {
            "disabled-icon": "mic_off"
          }
        },
        {
          "icon": "arrow_drop_down"
        }
      ],
      "camera": [
        {
          "icon": "videocam",
          "text": "mel_metapage.videoinput",
          "data": {
            "disabled-icon": "videocam_off"
          }
        },
        {
          "icon": "arrow_drop_down"
        }
      ],
      "handup": {
        "icon": "hand_gesture",
        "text": "mel_metapage.to_participate",
        "data": {
          "disabled-icon": "pan_tool"
        }
      },
      "share_screen": {
        "icon": "stop_screen_share",
        "text": "mel_metapage.diffuse",
        "data": { "disabled-icon": "screen_share" }
      },
      "moz": {
        "icon": "auto_awesome_mosaic",
        "text": "mel_metapage.mosaik"
      },
      "more": {
        "icon": "more_horiz",
        "text": "mel_metapage.more"
      },
      "hangup": {
        "icon": "call_end",
        "text": "mel_metapage.hangup"
      }
    }
  }
}
